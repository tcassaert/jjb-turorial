pipeline {
  agent any
  environment {
    BUILD_NUMBER = "${env.BUILD_NUMBER}"
    WORKSPACE    = "${env.WORKSPACE}"
  }
  parameters {
    string(name: 'package_name')
  }
  stages {
    stage('Get package') {
      steps {
        println("Downloading ${package_name} from registry")
      }
    }
  }
}
